<!doctype html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Title Of Site -->
    <title>Table Finder</title>
    <meta name="description" content="HTML Responsive Template for Restaurant Finder Based on Twitter Bootstrap 3.x.x" />
    <meta name="keywords" content="restaurant, dinner, lunch, eat, food, rice, dine, menu, dining, meal, cafe, breakfast" />
    <meta name="author" content="crenoveative">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Fav and Touch Icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/html/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/html/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/html/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/html/images/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="~/html/images/ico/favicon.png">

    <!-- CSS Plugins -->
    <link rel="stylesheet" type="text/css" href="~/html/bootstrap/css/bootstrap.min.css" media="screen">
    <link href="~/html/css/animate.css" rel="stylesheet" />
    <link href="~/html/css/main.css" rel="stylesheet">
    <link href="~/html/css/component.css" rel="stylesheet">

    <!-- CSS Custom -->
    <link href="~/html/css/style.css" rel="stylesheet">

    <!-- For your own style -->
    <link href="~/html/css/your-style.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body class="full-width-container transparent-header">
    <div id="introLoader" class="introLoading"></div>

    <!-- start Container Wrapper -->
    <div class="container-wrapper">
        @RenderBody()
    </div>
    <!-- end Container Wrapper -->
    <!-- start Back To Top -->
    <div id="back-to-top">
        <a href="#"><i class="fa fa-chevron-up"></i></a>
    </div>
    <!-- end Back To Top -->

    <div id="ajaxLoginModal" class="modal fade login-box-wrapper" data-width="500" data-backdrop="static" data-keyboard="false" tabindex="-1" style="display: none;"></div>

    <div id="ajaxRegisterModal" class="modal fade login-box-wrapper" data-width="500" data-backdrop="static" data-keyboard="false" tabindex="-1" style="display: none;">
    </div>

    <div id="ajaxForgotPasswordModal" class="modal fade login-box-wrapper" data-width="500" data-backdrop="static" data-keyboard="false" tabindex="-1" style="display: none;"></div>
        <!-- JS -->
        <script type="text/javascript" src="~/html/js/jquery-2.2.4.min.js"></script>
        <script type="text/javascript" src="~/html/js/jquery-migrate-1.4.1.min.js"></script>
        <script type="text/javascript" src="~/html/bootstrap/js/bootstrap.min.js"></script>
        <script type="text/javascript" src="~/html/js/SmoothScroll.min.js"></script>
        <script type="text/javascript" src="~/html/js/jquery.waypoints.min.js"></script>
        <script type="text/javascript" src="~/html/js/jquery.easing.1.3.js"></script>
        <script type="text/javascript" src="~/html/js/jquery.slicknav.min.js"></script>
        <script type="text/javascript" src="~/html/js/spin.min.js"></script>
        <script type="text/javascript" src="~/html/js/jquery.introLoader.min.js"></script>
        <script type="text/javascript" src="~/html/js/fancySelect.js"></script>
        <script type="text/javascript" src="~/html/js/bootstrap-rating.js"></script>
        <script type="text/javascript" src="~/html/js/select2.full.js"></script>
        <script type="text/javascript" src="~/html/js/slick.min.js"></script>
        <script type="text/javascript" src="~/html/js/jquery.placeholder.min.js"></script>
        <script type="text/javascript" src="~/html/js/ion.rangeSlider.min.js"></script>
        <script type="text/javascript" src="~/html/js/readmore.min.js"></script>
        <script type="text/javascript" src="~/html/js/bootstrap-modalmanager.js"></script>
        <script type="text/javascript" src="~/html/js/bootstrap-modal.js"></script>
        <script type="text/javascript" src="~/html/js/instagram.min.js"></script>
        <script type="text/javascript" src="~/html/js/customs.js"></script>
